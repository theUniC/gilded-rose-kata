{
    "name": "theunic/gilded-rose-kata",
    "description": "Port of the Gilded Rose Kata for C# (https://github.com/NotMyself/GildedRose)",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "authors": [
        {
            "name": "Bobby Johnson",
            "email": "bobby.johnson@gmail.com"
        },
        {
            "name": "Christian Soronellas",
            "email": "theunic@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-4": {
            "GildedRose\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\GildedRose\\Console\\": "tests/"
        }
    }
}
